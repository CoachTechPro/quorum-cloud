[
  {
    "name" : "privacy_engine",
    "image" : "${tx_privacy_engine_docker_image}",
    "essential" : true,
    "logConfiguration" : {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${quorum_log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "privacy_engine"
      }
    },
    "portMappings": [
      {
        "containerPort": 10000,
        "hostPort": 10000
      }
    ],
    "mountPoints" : {
      "sourceVolume" : "${shared_volume}",
      "containerPath" : "/quorum_shared"
    },
    "cmd": ${commands},
    "entryPoint" : ${entry_point}
  },
  {
    "name" : "quorum",
    "links" : [
      "privacy_engine"
    ],
    "image" : "${quorum_docker_image}",
    "essential" : true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${quorum_log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "quorum"
      }
    },
    "mountPoints" : {
      "sourceVolume" : "${shared_volume}",
      "containerPath" : "/quorum_shared"
    },
  }
]